<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>karma</ShortName>
  <Image width="27" height="27">data:image/png;base64,</Image>
  <Url type="application/x-suggestions+json" method="GET" template="https://api.karmasearch.org/search/autosuggest">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <Url type="text/html" method="GET" template="https://karmasearch.org/search">
    <Param name="q" value="{searchTerms}"/>
    <Param name="origin" value="firefoxandroid"/>
  </Url>
  <SearchForm>https://karmasearch.org</SearchForm>
</SearchPlugin>
