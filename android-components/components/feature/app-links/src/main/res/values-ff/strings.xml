    <!-- Opens the selected time -->
    <string name="mozac_feature_applinks_confirm_dialog_confirm">Uddit</string>
    <!-- Cancels the prompt -->
    <string name="mozac_feature_applinks_confirm_dialog_deny">Haaytu</string>
